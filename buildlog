[INFO 2025-08-13 21:03:45.804887] Sending prompt: Please generate a heavily commented version of this file with a document summary that includes use cases. #!/bin/bash
set -e

echo "📦 Starting PiKit Snapshot Build..."

# Step 1: Create a snapshot output directory
SNAPSHOT_NAME="pikit_snapshot_$(date +%Y-%m-%d_%H-%M-%S)"
SNAPSHOT_DIR="$HOME/pikit-snapshots"
BUILD_DIR="pikit_snapshot_build"

mkdir -p "$SNAPSHOT_DIR"
rm -rf "$BUILD_DIR"
mkdir "$BUILD_DIR"

echo "🔁 Copying project source into $BUILD_DIR..."
rsync -av --exclude='__pycache__' --exclude='.git' . "$BUILD_DIR/"

echo "🗂️ Copying current storage folder into snapshot build..."
cp -r storage "$BUILD_DIR/"

# Step 2: Enter the build directory
cd "$BUILD_DIR"

echo "🐍 Creating virtualenv for isolated build..."
python3 -m venv snapshot-env
source snapshot-env/bin/activate

echo "📚 Installing dependencies..."
pip install --upgrade pip
pip install -r requirements.txt
pip install pyinstaller

echo "📄 Building standalone PiKit binary with full storage included..."
pyinstaller main.py --onefile --clean --name "$SNAPSHOT_NAME"

echo "📁 Moving final binary to $SNAPSHOT_DIR/"
mv "dist/$SNAPSHOT_NAME" "$SNAPSHOT_DIR/$SNAPSHOT_NAME.bin"

echo "✅ Snapshot complete: $SNAPSHOT_DIR/$SNAPSHOT_NAME.bin"



